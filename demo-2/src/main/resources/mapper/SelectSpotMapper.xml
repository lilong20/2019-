<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.SelectSpotMapper">
 <select id="selectSpotByViewname" parameterType="java.lang.String" resultType="com.example.demo.model.SelectSpot">
        select * from spot
        where
       viewname =#{viewname}
 </select>
 </mapper>