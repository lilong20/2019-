<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC   
    "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.swjtuhc.demo.mapper.goodmapper">
	<select id="selectGoodBygoodsname" parameterType="java.lang.String" resultType="edu.swjtuhc.demo.mode.Good">
		select * from good
		where
		goodsname=#{goodsname}
	</select>

</mapper>